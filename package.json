{
  "name": "dazureui",
  "version": "1.0.0",
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "build-node": "babel app -d build --copy-files",
    "build-webpack": "webpack --mode production --env.production",
    "start": "node build/server/index.js",
    "client": "nodemon -w webpack.config.js -x webpack-dev-server -- --mode development --env.development",
    "server": "nodemon --use_strict app/server --exec babel-node",
    "dev": "concurrently 'npm run server' 'npm run client'",
    "prettier:fix": "prettier --print-width 80 --single-quote --trailing-comma es5 --write index.js **/*.js",
    "precommit": "lint-staged",
    "lint": "eslint .",
    "lint:fix": "eslint lib/ --fix",
    "format": "npm run lint:fix && npm run prettier:fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "npm run format",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/node": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "axios": "0.18.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.4",
    "babel-plugin-transform-class-properties": "6.24.1",
    "body-parser": "1.18.3",
    "clean-webpack-plugin": "0.1.19",
    "cors": "2.8.4",
    "eslint": "5.7.0",
    "eslint-plugin-react": "7.11.1",
    "express": "4.16.4",
    "html-webpack-plugin": "3.2.0",
    "moment": "2.22.2",
    "react": "16.5.2",
    "react-bootstrap": "0.32.4",
    "react-dom": "16.5.2",
    "react-html-parser": "^2.0.2",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "webpack": "4.20.2",
    "webpack-merge": "4.1.4"
  },
  "devDependencies": {
    "concurrently": "4.0.1",
    "eslint-config-prettier": "3.1.0",
    "husky": "1.1.3",
    "lint-staged": "8.0.4",
    "nodemon": "1.18.4",
    "prettier": "1.14.3",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.9"
  }
}
